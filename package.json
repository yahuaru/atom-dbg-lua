{
  "name": "dbg-lua",
  "main": "./lib/dbg-lua",
  "version": "0.1.0",
  "description": "An interactive Lua debugger for Atom",
  "keywords": [
    "debug",
    "debugger"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yahuaru/atom-dbg-lua.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "dbg",
    "output-panel",
    "Defold",
    "lua"
  ],
  "dependencies": {
    "atom-package-deps": "^4.0.1"
  },
  "providedServices": {
    "dbgProvider": {
      "description": "Provides a dbg lua debugger",
      "versions": {
        "1.1.0": "provideDbgProvider"
      }
    }
  },
  "consumedServices": {
    "dbg": {
      "versions": {
        "^1.0.0": "consumeDbg"
      }
    },
    "output-panel": {
      "versions": {
        "^1.0.0": "consumeOutputPanel",
        "^0.1.0": "consumeOutputPanel"
      }
    }
  }
}
